<views:MvxWpfView xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
                  xmlns:controls="clr-namespace:DeltaClient.WPF.Controls"
                  xmlns:mvx="clr-namespace:MvvmCross.Platforms.Wpf.Binding;assembly=MvvmCross.Platforms.Wpf"
                  x:Class="DeltaClient.WPF.Views.BinaryTreeView"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:local="clr-namespace:DeltaClient.WPF.Views"
                  mc:Ignorable="d" Background='Aquamarine' 
                  Height="400" Width="800">

  <controls:BinaryTreeControl>
    <Ellipse Width="50"
             Height="50"
             Fill="Blue" />
    <Ellipse Width="50"
             Height="50"
             Fill="Green" />
    <Ellipse Width="50"
             Height="50"
             Fill="Yellow" />
    <Ellipse Width="50"
             Height="50"
             Fill="Red"/>
    <Ellipse Width="50"
             Height="50"
             Fill="DarkOrange" />
    <Ellipse Width="50"
             Height="50"
             Fill="SaddleBrown"/>
    <Ellipse Width="50"
             Height="50"
             Fill="Wheat"/>
    
  </controls:BinaryTreeControl>
  
</views:MvxWpfView>
